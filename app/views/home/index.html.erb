<section id="MyProjects" class="projects">
  <div class="container">
    <h2 class="heading heading-sec section-heading">
      <span>Projects</span>
    </h2>
    <div class="projects-list">

      <% @projects.each do |project| %>
        <div class="projects-list__project">
          <div class="projects-list__img-cont">
            <%= image_tag project.images.first, class: "projects-list__img" %>
          </div>

          <h3 class="projects-list__heading">
            <%= project.title %>
          </h3>

          <p class="text-tertiary projects-list__text-tertiary">
            <%= project.description %>
          </p>

          <%= link_to "More Detail", project_detail_path(project), class: "btn btn--theme projects-list__btn" %>


        </div>
      <% end %>

    </div>
  </div>
</section>
<section class="contact">
  <div class="container">
    <h2 class="heading heading-sec section-heading contact__heading">
      <span>Contact</span>
    </h2>
    <div class="contact__form-container">
      <form action="#" class="contact__form">
        <div class="contact__form-field">
          <input
              placeholder="Name"
              type="text"
              class="contact__form-input"
              />
        </div>
        <div class="contact__form-field">
          <input
              placeholder="Email"
              type="text"
              class="contact__form-input"
              />
        </div>
        <div class="contact__form-field">
          <input
              placeholder="Phone Number"
              type="text"
              class="contact__form-input"
              />
        </div>
        <div class="contact__form-field">
              <textarea
                  name=""
                  id=""
                  cols="30"
                  rows="10"
                  class="contact__form-input"
                  placeholder="Message"
              ></textarea>
        </div>
        <button class="btn btn--primary contact__btn">Submit</button>
      </form>
    </div>
  </div>
</section>
